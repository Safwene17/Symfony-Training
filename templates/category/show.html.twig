{% extends 'base.html.twig' %}

{% block title %}Category{% endblock %}

{% block body %}

    <a href="{{ path('app_category_index') }}">back to list</a>
    <h1>Category</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ category.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ category.description }}</td>
            </tr>
        </tbody>
    </table>





{% endblock %}
